{
  "name": "bmfaudio-server",
  "version": "1.0.1",
  "description": "File server for BMFAudio device records menage.",
  "author": "Pavel Baranau",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec \"node -r ts-node/register --inspect-brk\" server/index.ts | pino-pretty",
    "lint": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet",
    "lint:fix": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet --fix",
    "test": "mocha -r ts-node/register test/**/*.ts --exit",
    "test:debug": "mocha -r ts-node/register --inspect-brk test/**/*.ts --exit",
    "postinstall": "npm run compile"
  },
  "engines": {
    "node": "12.16.x",
    "npm": "6.14.x"
  },
  "dependencies": {
    "@types/archiver": "^3.1.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/multer": "^1.4.4",
    "@types/shelljs": "^0.8.8",
    "archiver": "^5.0.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^3.16.11",
    "moment": "^2.28.0",
    "multer": "^1.4.2",
    "pino": "^6.5.1",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/express": "^4.17.7",
    "@types/mocha": "^8.0.3",
    "@types/pino": "^6.3.0",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.1.1",
    "nodemon": "^2.0.4",
    "pino-pretty": "^4.1.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
